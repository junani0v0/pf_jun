<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.pf.www.forum.notice.dao.mybatis.NoticeRepository">

    <!-- insert 타입 int라 뭘 넣어줄수 없어 selectKey사용 -->
    <insert id="addBoard" parameterType="hashMap">
    	<selectKey resultType="String" keyProperty="boardSeq" keyColumn="board_seq" order="AFTER" >
    	SELECT LAST_INSERT_ID()
    	</selectKey>
    	
    	INSERT INTO forum.`member` 
    	(member_seq, member_id, passwd, member_nm, email, auth_yn, pwd_chng_dtm, join_dtm)
    	VALUES(0, '', '', '', '', 'N', '', '')
    </insert>
    
</mapper>